{
  "manifest_version": 3,
  "name": "Eisenhower matrix todo",
  "version": "2.0",
  "description": "A todo list that adapted eisenhower matrix",

  "permissions": ["storage", "favicon", "search"],

  "action": {
    "default_icon": "favicon.png"
  },

  "chrome_url_overrides": {
    "newtab": "index.html"
  },

  "background": {
    "service_worker": "scripts/background.js"
  }
}

{
  "name": "Eisenhower matrix todo",
  "version": "1.0",
  "description": "A todo list that adapted eisenhower matrix",
  "background": {
    "scripts": ["scripts/background.js"],
    "persistent": false
  },
  "content_scripts": {
    "js": ["scripts/content.js"]
  },
  "permissions": ["storage"],  
  "browser_action": {    
    "default_icon": "favicon.png",    
    "default_popup": "./src/extension/popup.html"  
  },
  "manifest_version": 2
}